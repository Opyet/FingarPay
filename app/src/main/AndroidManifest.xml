<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fingarpay">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".HomeScreenActivity" />
        <activity android:name=".ReceivePaymentActivity" />
        <activity android:name=".SuccessReportActivity" />
        <activity android:name=".FailedReportActivity" />
        <activity android:name=".TransferActivity" />
        <activity android:name=".HistoryTransactionActivity" />
        <activity android:name=".SetUpFingerScan"></activity>
    </application>

</manifest>